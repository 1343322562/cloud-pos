.goodsCar {
  background-color: #f5f5f5;
  color: #333333;
  font-size: 30rpx;
  height: 100vh;
}
.car-top-bg {
  background-color: #fecc38;
  background: linear-gradient(to bottom, #ffaf11, #feca0a);
  height: 250rpx;
  position: absolute;
  width: 100%;
}
.we-bg {
  height: 80rpx;
}
.car-edit-adress-box {
  position: relative;
  width: 94%;
  margin: 0 3%;
}
.car-edit-adress-box .car-sign {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.car-edit-adress-box .at-list {
  padding: 23rpx 15rpx;
  border-radius: 16rpx;
}
.car-edit-adress-box .at-list .at-list__item::after {
  border-width: 0;
}
.car-edit-adress-box .at-list .item-thumb {
  color: #666666;
  height: 42rpx;
  width: 42rpx;
}
.car-edit-adress-box .at-list .item-content__info-title {
  font-size: 29rpx;
  letter-spacing: 1rpx;
  font-weight: bold;
  text-align: center;
}
.car-edit-adress-box .at-list .at-icon-chevron-right {
  color: #666666;
  font-size: 39rpx;
}
.goods-car-detail-box {
  border-top: 2PX solid #feb70f;
  margin-top: 35rpx;
  min-height: 80vh;
  width: 100%;
  background-color: #fff;
  position: relative;
  padding-bottom: 300rpx;
}
.goods-car-detail-box .good-info-item {
  font-size: 26rpx;
  width: 100%;
  border-bottom: 1PX solid #eee;
  display: flex;
  align-items: center;
}
.goods-car-detail-box .good-info-item .checkout-selected {
  padding: 30rpx 18rpx;
  width: 55rpx;
}
.goods-car-detail-box .good-info-item .goods-Img {
  height: 170rpx;
  width: 140rpx;
}
.goods-car-detail-box .good-info-item .goods-props {
  display: flex;
  flex-direction: column;
  height: 225rpx;
  width: 460rpx;
  padding-left: 18rpx;
  justify-content: space-around;
}
.goods-car-detail-box .good-info-item .goods-props .props {
  display: flex;
  justify-content: space-between;
}
.goods-car-detail-box .good-info-item .goods-props .props .price {
  color: #fe680a;
  font-size: 32rpx;
  font-weight: bold;
}
.goods-car-detail-box .good-info-item .goods-props .props .swicth .goodsCarNum {
  display: inline-block;
  width: 60rpx;
  vertical-align: top;
  font-size: 28rpx;
  margin: 0 5rpx;
  text-align: center;
}
.goods-car-detail-box .good-info-item .goods-props .props .swicth .addIcon {
  font-size: 6rpx !important;
  color: #333333;
  -webkit-transform: rotateZ(45deg);
          transform: rotateZ(45deg);
  background: #feb40f;
  border-radius: 50%;
  font-weight: bold;
}
.goods-car-detail-box .good-info-item .goods-props .props .swicth .subIcon,
.goods-car-detail-box .good-info-item .goods-props .props .swicth .addIcon {
  padding: 9rpx;
}
.goods-car-detail-box .res-info-box {
  display: flex;
  justify-content: space-between;
  padding: 12rpx 3%;
  color: #333333;
  align-items: center;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  width: 94%;
  box-shadow: 0 0 20rpx 0 #ccc;
  z-index: 10;
}
.goods-car-detail-box .res-info-box .res-info {
  display: flex;
  justify-content: center;
  align-items: center;
}
.goods-car-detail-box .res-info-box .res-info .selected-all {
  font-size: 28rpx;
  display: flex;
  width: 120rpx;
  justify-content: space-between;
  align-items: center;
}
.goods-car-detail-box .res-info-box .res-info .selected-all .none {
  display: none;
}
.goods-car-detail-box .res-info-box .price-info {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  letter-spacing: 3rpx;
  justify-content: center;
  align-content: space-between;
  height: 96rpx;
}
.goods-car-detail-box .res-info-box .price-info .after {
  font-size: 32rpx;
}
.goods-car-detail-box .res-info-box .price-info .after .span {
  color: #fe680a;
  font-weight: bold;
  letter-spacing: 0;
  width: 115rpx;
  display: inline-block;
  white-space: no-wrap;
}
.goods-car-detail-box .res-info-box .price-info .before {
  font-size: 24rpx;
}
.goods-car-detail-box .res-info-box .price-info .before .span {
  letter-spacing: 0;
}
.goods-car-detail-box .res-info-box .res-btn {
  margin: 0;
  font-size: 33rpx;
  padding: 0 25rpx;
  height: 80rpx;
  line-height: 77rpx;
  margin-right: -20rpx;
  border-radius: 60rpx;
}
.goods-car-detail-box .res-info-box .goods-delete {
  height: 58rpx;
  width: 58rpx;
  margin-right: 15rpx;
}