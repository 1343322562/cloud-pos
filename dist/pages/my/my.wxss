.my .at-nav-bar__title {
  font-size: 29rpx;
  height: 34rpx;
}
.my {
  font-size: 29rpx;
  width: 100%;
  height: 100vh;
  background-color: #f3f3f3;
  overflow: hidden;
}
.orderClickBox {
  position: absolute;
  height: 80rpx;
  width: 210rpx;
  right: 0;
  top: 0;
  background-color: transparent;
}
.at-card,
.at-card__header,
.at-tab-bar::after,
.at-nav-bar::after {
  border-width: 0;
}
.at-card__header {
  padding: 30rpx 27rpx;
}
.my-header-card-box {
  margin-top: 3%;
  height: 230rpx;
}
.at-tag--active {
  background-color: #fff6e6;
  color: #fda308;
  border: 0;
  margin-bottom: -5rpx;
}
.my-user-order-card {
  margin-top: 3%;
  position: relative;
}
.at-card__header-title {
  font-size: 31rpx;
  font-weight: bold;
  color: #333333;
}
.at-card__header-extra {
  color: #999999;
  font-size: 24rpx;
}
.at-tab-bar {
  width: 103%;
  background-color: transparent;
  -webkit-transform: translateX(-3%);
          transform: translateX(-3%);
}
.at-tab-bar__inner-img {
  margin-bottom: 15rpx;
}
.at-card__content {
  padding: 0 0 16rpx 0;
}
.at-tab-bar__title {
  font-size: 26rpx;
  color: #333333;
  letter-spacing: 1rpx;
}
.at-tab-bar__item {
  padding-top: 0;
  padding-bottom: 0;
  display: flex;
  flex-direction: column;
}
.at-tab-bar__item--active {
  color: #333333;
}
.my-List-card-box {
  height: 100vh;
  width: 94%;
  background-color: white;
  margin: 3% 3%;
}
.my .at-list__item-thumb {
  width: 48rpx;
  height: 48rpx;
}
.my .item-content__info-title {
  font-size: 28rpx;
}
.at-list__item {
  padding: 32rpx 30rpx;
}
.my .at-tab-bar__inner-img {
  height: 48rpx;
  width: 46rpx;
}